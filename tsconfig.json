{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
   // "strict": true,
    "target": "esnext",
    "module": "esnext",
   // "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react-native"
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
